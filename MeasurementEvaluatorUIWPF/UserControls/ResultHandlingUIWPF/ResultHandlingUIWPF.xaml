<base:UserControlBase x:Class="MeasurementEvaluatorUIWPF.UserControls.ResultHandlingUIWPF.ResultHandlingUIWPF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MeasurementEvaluatorUIWPF.UserControls.ResultHandlingUIWPF"
             xmlns:base="clr-namespace:MeasurementEvaluatorUI.Base"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.Resources>
        <Style x:Key="ButtonStyle" TargetType="Button" >
            <Setter Property="Margin" Value="10"/>
        </Style>
    </UserControl.Resources>
    

    <StackPanel Orientation="Horizontal">
        <Button Content="Save Results" Style="{StaticResource ButtonStyle}"/>
        <Button Content="Exit" Style="{StaticResource ButtonStyle}"/>
    </StackPanel>
           
</base:UserControlBase>
